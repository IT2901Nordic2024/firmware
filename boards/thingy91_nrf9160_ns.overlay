/* Enable uart1 for tracing. */
&uart1 {
	status = "okay";
	current-speed = < 1000000 >;
	hw-flow-control;
};

/ {
	chosen {
		nordic,modem-trace-uart = &uart1;
	};
	
	/* ... */
	/* These aliases are provided for compatibility with samples */
	aliases {
	/* ... */
		sw0 = &button0;
	};
	
};

